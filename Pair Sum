//Pair Sum

//----->>> Two pointer method (O(n))

#include <bits/stdc++.h>
using namespace std;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    int n;
    cin >> n;

    vector<int> v, ans;

    for(int i = 0; i < n; i++) {
        int x;
        cin >> x;
        v.push_back(x);
    }

    sort(v.begin(), v.end());

    int target;
    cin >> target;

    int i = 0, j = n - 1;
    while(i < j) {
        int sum = v[i] + v[j];

        if(sum > target) {
            j--;
        }
        else if(sum < target) {
            i++;
        }
        else {
            ans.push_back(v[i]);
            ans.push_back(v[j]);
            break;
        }
    }

    if(ans.size() == 2) {
        cout << ans[0] << "," << ans[1] << endl;
    } else {
        cout << "No pair found\n";
    }

    return 0;
}
